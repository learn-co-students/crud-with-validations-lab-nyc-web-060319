<% if @song.errors.any? %>
  <h2>
    <%= pluralize(@song.errors.count, "error") %>
    prohibited this student from being saved:
  </h2>
  <ul>
    <% @song.errors.full_messages.each do |message|%>
      <li>
        <%= message %>
      </li>
    <% end%>
  </ul>
<% end %>
<%=form_for @song do |f|%>
  <%=f.label :title%>
  <%= f.text_field :title %> <br>
  <br>
  <%= f.label :released %>
  <%=f.check_box :released %> <br>
  <br>
  <%= f.label :release_year %>
  <%= f.number_field :release_year %><br>
  <br>
  <%=f.label :artist_name%>
  <%= f.text_field :artist_name %> <br>
  <br>
  <%=f.label :genre%>
  <%= f.text_field :genre %> <br>
  <br>
  <%= f.submit "Make a song" %>
<% end%>
